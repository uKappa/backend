{
  "name": "@qualweb/qw-page",
  "version": "0.2.21",
  "description": "Utilities module for qualweb",
  "main": "dist/qw-page.bundle.js",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "tsc": "tsc",
    "test": "mocha",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier src/**/*.ts --write",
    "prebuild": "rimraf prebuild  && tsc --build tsconfig.prod.json",
    "build": "npm run prebuild && rimraf dist && webpack --mode production && rimraf prebuild",
    "docs": "typedoc --out docs src/index.ts",
    "prepublishOnly": "npm run build"
  },
  "homepage": "https://github.com/qualweb/qw-page#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qualweb/qw-page.git"
  },
  "bugs": {
    "url": "https://github.com/qualweb/qw-page/issues"
  },
  "keywords": [
    "qualweb",
    "accessibility",
    "qualweb-page"
  ],
  "author": "Jo√£o Vicente",
  "license": "ISC",
  "devDependencies": {
    "@qualweb/dom": "0.2.10",
    "@qualweb/types": "0.7.26",
    "@tsconfig/recommended": "^1.0.3",
    "@types/mocha": "^10.0.6",
    "chai": "^4.4.1",
    "eslint": "^8.56.0",
    "mocha": "^10.2.0",
    "prettier": "^3.1.1",
    "puppeteer": "^21.6.1",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.2",
    "typedoc": "^0.25.6",
    "typescript": "^4.7.4",
    "webpack": "^5.89.0",
    "webpack-cli": "^4.7.2"
